{
  "name": "ooxml-templater",
  "version": "0.0.1",
  "description": "no-nonsense docx and xlsx templater",
  "scripts": {
    "build": "npx rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "dev": "npm run build -- --watch",
    "cli": "tsx scripts/cli.ts",
    "test": "vitest"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./xlsx": {
      "require": "./dist/xlsx.js",
      "types": "./dist/xlsx.d.ts",
      "import": "./dist/xlsx.mjs",
      "default": "./dist/xlsx.mjs"
    },
    "./docx": {
      "require": "./dist/docx.js",
      "types": "./dist/docx.d.ts",
      "import": "./dist/docx.mjs",
      "default": "./dist/docx.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyxan23/ooxml-templater-js.git"
  },
  "keywords": [
    "ooxml",
    "templater",
    "parser"
  ],
  "author": "iyxan23",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyxan23/ooxml-templater-js/issues"
  },
  "homepage": "https://github.com/iyxan23/ooxml-templater-js#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^22.1.0",
    "prettier": "^3.3.3",
    "rollup": "^4.20.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^7.1.2",
    "rollup-plugin-swc3": "^0.11.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "vitest-mock-extended": "^2.0.0"
  },
  "sideEffects": false,
  "dependencies": {
    "@zip.js/zip.js": "^2",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^4"
  },
  "peerDependencies": {
    "zod": "^3"
  }
}
